-average2 o
#get the average of 2 numbers
%BLOCKINPUTS
number
number
%VARIABLES
total-number
%NODES
o,/0/,/1/,[total],+,1
@o,[total],(number-2),/blockoutput/,/

-absolute o
#get the absolute value of a number
%BLOCKINPUTS
number
%VARIABLES
%NODES
c,/0/,(number-0),<,1,2
@o,/0/,(number--1),/blockoutput/,*
@o,/0/,(number-0),/blockoutput/,+